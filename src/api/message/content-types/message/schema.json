{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "singularName": "message",
    "pluralName": "messages",
    "displayName": "Message"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true,
      "maxLength": 300,
      "minLength": 20
    },
    "enqType": {
      "type": "enumeration",
      "enum": [
        "AR",
        "EN",
        "FR",
        "GR"
      ],
      "required": true
    },
    "subject": {
      "type": "string",
      "required": true
    }
  }
}
